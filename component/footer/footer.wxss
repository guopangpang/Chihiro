.foot_containt{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: space-around;
  background-color: #f7f7f7;
  z-index: 1000;
}
.foot_containt .foot_item{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center
}
.foot_containt .foot_item .foot_item_image{
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 10rpx;
}
.foot_containt .foot_item .foot_item_text{
  font-size: 24rpx
}
.color_01A6F6{
  color: #01A6F6;
}